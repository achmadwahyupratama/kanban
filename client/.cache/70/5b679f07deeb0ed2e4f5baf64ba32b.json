ask Status\">\r\n        </div>\r\n        <div class=\"task-manager\">\r\n            <button class=\"btn btn-primary\" type=\"submit\">update</button>\r\n            <button @click.prevent=\"cancelUpdate\"  class=\"btn btn-danger\" >Cancel</button>\r\n        </div>\r\n    </form>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"Task\",\r\n    data(){\r\n        return{\r\n            showEditForm: false,\r\n            updatedTitle: '',\r\n            updatedDescription: '',\r\n            updatedStatus: ''\r\n        }\r\n    },\r\n    props: ['task'],\r\n    methods: {\r\n        deleteTask(taskId){\r\n            this.$emit('destroyTask', taskId)\r\n        },\r\n        showsEditForm(task){\r\n            this.updatedTitle = task.title,\r\n            this.updatedDescription = task.description,\r\n            this.updatedStatus = task.status,\r\n            this.showEditForm = true\r\n        },\r\n        submitPutTask(task){\r\n            let updatedTask = {\r\n                id: task.id,\r\n                title: this.updatedTitle,\r\n                description: this.updatedDescription,\r\n                status: this.updatedStatus,\r\n                CategoryId: task.CategoryId            \r\n            }\r\n            // console.log(updatedTask);\r\n            this.$emit('putTask', updatedTask)\r\n            this.showEditForm = false\r\n        },\r\n        cancelUpdate(){\r\n            this.showEditForm = false\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"2650143094cac9439088e00dbb4be6bb","cacheData":{"env":{}}}